{
    "manifest_version":3,
    "name":"Youtube Summariser",
    "description":"An extension to summarize youtube videos using the transcript",
    "version":"1.0",
    "permissions":["activeTab","declarativeContent"],
    "action": {
        "default_title": "Summarise this video",
        "default_icon": {
            "16": "images/images.png",
            "32": "images/images.png",
            "48": "images/images.png",
            "128": "images/images.png"
        },
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/images.png",
        "32": "images/images.png",
        "48": "images/images.png",
        "128": "images/images.png"
    }

}